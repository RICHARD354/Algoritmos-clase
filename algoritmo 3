#include <stdio.h>#include <stdio.h>

int main(){
    int n = 8;
    int i;
    int j;
    for(i=1; i<=n; i++){
        for(j=1; j<=n-i; j++){
           printf(" ");
           printf(" ");
        }
        for(j=i; j<=2*i-1; j++){
            if(j>=0&&j<=9){
                printf("%d ", j);
            }
            
        }
       printf("\n");
    }

    return 0;
}
